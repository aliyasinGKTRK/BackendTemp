@model Entity.About
@{
    ViewData["Title"] = "AboutAdd";
}

<h1>AboutAdd</h1>


<div class="row">
    <div class="col-md-8">
        <form asp-controller="Admin" asp-action="AboutAdd" method="post">
            <div class="form-group row">
                <label asp-for="AboutTitle" class="col-md-2 col-form-label"></label>
                <div class="col-md-10">
                    <input class="form-control"  asp-for="AboutTitle" value="" />

                </div>

            </div>
            <div class="form-group row">
                <label asp-for="Description" class="col-md-2 col-form-label"></label>
                <div class="col-md-10">
                    <input class="form-control"  asp-for="Description" value="" />

                </div>

            </div>
    

              <div class="form-group row">
                <div class="col-md-10 offset-md-2">
                    <button type="submit" v class="btn btn-primary">Save Product</button>

                </div>

            </div>
        </form>
    </div>
</div>
